{
    executable: {{{ROOT_DIR}}}/bin/tezpeak
    args: [
        {{#configuration.STARTUP_ARGS}}
        {{.}}
        {{/configuration.STARTUP_ARGS}}
    ]
    environment: {
        HOME: {{{ROOT_DIR}}}
        TEZPEAK_CONFIG_FILE: {{{ROOT_DIR}}}/config.hjson
        TEZBAKE_HOME: {{{model.TEZBAKE_HOME}}}
    }
    working_directory: {{{ROOT_DIR}}}/data
    restart: always
    restart_delay: 20
    user: {{{model.USER}}}
}